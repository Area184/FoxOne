  <PageEntity>
    <Id>UserSelector</Id>
    <RentId>1</RentId>
    <Title>人员选择器</Title>
    <CssClass>easyui-layout</CssClass>
    <Style>        .user-item {
            border: 1px solid #CCC;
            background-color: #EEE;
            padding: 10px;
            margin-top: 10px;
            margin-left: 10px;
            width: 150px;
            border-radius: 5px;
            cursor: pointer;
            float: left;
        }

            .user-item p {
                border-bottom: 1px dashed #CCC;
                font-weight: bold;
                padding-bottom: 5px;
            }

        .user-item-checked {
            background: white url(../../images/default/success.png) 140px 5px no-repeat;
        }

        .user-item-checked-lock {
            background: #EEEFFF url(../../images/default/success.png) 140px 5px no-repeat;
        }</Style>
    <StartUpScript>$("#UserSelectorTree").bind("nodeClick",function(e){
var setting=foxOne.setting("UserSelectorRepeater");
setting.DepartmentId=e.id;
foxOne.refresh("UserSelectorRepeater");
});

$("body").on("click",".user-item",function(e){
var div=$(e.target).closest(".user-item");
var hasClass = div.hasClass("user-item-checked");
var d=$(this).data();
var param=foxOne.getQueryString();
foxOne.dataService("/Entity/UserRole",{UserId:d.id,RoleId:param.RoleId,Add:!hasClass},function(res){
if(hasClass){
div.removeClass("user-item-checked");
}
else{
div.addClass("user-item-checked");
}
});
//$("#"+param._input_id,window.parent.document).val(d.id);
//$("#"+param._input_text,window.parent.document).val(d.name);
});</StartUpScript>
